{
    "name": "cookie-banner-gdpr-request",
    "manifest_version": 3,
    "description": "Extract partners from cookie banners to request personal data (GDPR)",
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "host_permissions": [
        "https://www.paypal.com/"
    ],
    "permissions": [
        "offscreen",
        "storage",
        "tabs"
    ],
    "version": "0.3"
}
